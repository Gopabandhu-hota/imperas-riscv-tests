# Description: Makefrag for RV32IC compliance tests

rv32ic_sc_tests = \
	I-C-ADD-01 \
	I-C-MV-01 \
	I-C-ADDI-01 \
	I-C-LI-01 \
	I-C-LUI-01 \
	I-C-AND-01 \
	I-C-OR-01 \
	I-C-SUB-01 \
	I-C-XOR-01 \
	I-C-ANDI-01 \
	I-C-BEQZ-01 \
	I-C-BNEZ-01 \
	I-C-J-01 \
	I-C-JALR-01 \
	I-C-JR-01 \
	I-C-LW-01 \
	I-C-LWSP-01 \
	I-C-SLLI-01 \
	I-C-SRAI-01 \
	I-C-SRLI-01 \
	I-C-SW-01 \
	I-C-SWSP-01 \
	I-C-EBREAK-01 \
	I-C-NOP-01 \

rv32ic_tests = $(addsuffix .elf, $(rv32ic_sc_tests))

target_tests += $(rv32ic_tests)

